Class {
	#name : #DSHistogram,
	#superclass : #DSChart,
	#instVars : [
		'collection',
		'values'
	],
	#category : #'Dashboard-Charts-Domain'
}

{ #category : #description }
DSHistogram class >> dashboardDescription [

	^ DSChartDescription new
			kind: DSNumber;
			populationSize: (DSRange between: 10 and: 10000);
			yourself
	
]

{ #category : #examples }
DSHistogram class >> example [
	<script>
	"example04VerticalStack"
	
	| ch |
	ch := self new. 
	^ ch draw open 
	
]

{ #category : #drawing }
DSHistogram >> draw [

	| plot |
	plot := RSHistogramPlot new x: values.
	c addPlot: plot.
	c addDecoration: RSVerticalTick new.
	c addDecoration: RSHorizontalTick new.
	^ c
]

